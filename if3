uses Robot;

begin
  Task('if3');
  if (FreeFromDown and FreeFromLeft) then 
  begin
    Down; Left; 
  end
    else
  if (FreeFromUp and FreeFromRight) then 
  begin
    Up; Right;
  end;
  
  if (FreeFromUp and FreeFromLeft) then 
  begin
    Up; Left; 
  end
  else
  if (FreeFromDown and FreeFromRight) then 
  begin
    Down; Right; 
  end;
  Paint;
end.

