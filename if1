///////1 вариант
##
uses Robot;
Task('if1'); 

Right;
if WallFromRight and WallFromUp then 
begin
  Down;Right; Up; Paint;
end;

if WallFromRight and WallFromDown then 
begin
  Up; Right; Down; Paint;
end; 
///////////2 вариант
Right; 
  if WallFromUp then 
  begin
    Down;
    Right;
    Up;
    Paint;
    end;
 if WallFromDown then 
  begin
    Up;
    Right;
    Down;
    Paint;
    end; 
end.
